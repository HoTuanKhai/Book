                    <td>{{ s.Book_id }}</td>
                    <td>
                        {#<a href="{{ path('s_details', {'id': s.id}) }} " class='btn btn-success'>View</a>#}
                        {#<a href="{{ path('s_edit', {'id': s.id}) }}" class='btn btn-default'>Edit</a>#}
                        {#<a href="{{ path('s_delete', {'id': s.id}) }}" class='btn btn-danger'>Delete</a>#}
                        <a href="{{ path('s_details', {'id': s.id}) }} " class='btn btn-success'>View</a> 
                        <a href="#" class='btn btn-default'>Edit</a>
                        <a href="#" class='btn btn-danger'>Delete</a>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}